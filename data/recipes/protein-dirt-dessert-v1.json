      "instructions": [
        {
          "group": "Stabilized Whipped Cottage Cheese Base",
          "componentId": "stabilized-whipped-base",
          "steps": [
            {
              "step": 1,
              "text": "Bloom gelatin in cold water for 2–3 minutes."
            },
            {
              "step": 2,
              "text": "Heat gently until just liquid (do not boil)."
            },
            {
              "step": 3,
              "text": "Blend cottage cheese, milk, vanilla, sweetener, and salt until completely smooth."
            },
            {
              "step": 4,
              "text": "With blender running, drizzle in gelatin."
            },
            {
              "step": 5,
              "text": "Chill mixture 20–30 minutes."
            },
            {
              "step": 6,
              "text": "Whip with hand mixer or food processor until light and airy."
            }
          ]
        },
        {
          "group": "Chocolate Pudding Whipped Mousse",
          "componentId": "chocolate-mousse-layer",
          "steps": [
            {
              "step": 7,
              "text": "Whisk pudding mix with milk for 2 minutes."
            },
            {
              "step": 8,
              "text": "Let thicken for 5 minutes."
            },
            {
              "step": 9,
              "text": "Gently fold in whipped cottage cheese base."
            }
          ]
        },
        {
          "group": "Vanilla Pudding Whipped Mousse",
          "componentId": "vanilla-mousse-layer",
          "steps": [
            {
              "step": 10,
              "text": "Whisk pudding mix with milk for 2 minutes."
            },
            {
              "step": 11,
              "text": "Let thicken for 5 minutes."
            },
            {
              "step": 12,
              "text": "Fold in whipped cottage cheese base and almond extract."
            }
          ]
        },
        {
          "group": "Baked Oreo-Style Crumb Topping",
          "componentId": "oreo-style-crumb",
          "steps": [
            {
              "step": 13,
              "text": "Preheat oven to 165°C (325°F)."
            },
            {
              "step": 14,
              "text": "Mix all dry ingredients."
            },
            {
              "step": 15,
              "text": "Stir in melted fat and vanilla."
            },
            {
              "step": 16,
              "text": "Spread thin on parchment-lined tray."
            },
            {
              "step": 17,
              "text": "Bake 8–10 minutes, stirring once."
            },
            {
              "step": 18,
              "text": "Cool completely, then crumble finely."
            }
          ]
        },
        {
          "group": "Assemble & Chill",
          "steps": [
            {
              "step": 19,
              "text": "Spoon chocolate mousse into serving cups.",
              "stepType": "assemble"
            },
            {
              "step": 20,
              "text": "Layer vanilla mousse on top.",
              "stepType": "assemble"
            },
            {
              "step": 21,
              "text": "Finish with Oreo-style crumb topping.",
              "stepType": "assemble"
            },
            {
              "step": 22,
              "text": "Chill at least 1 hour before serving.",
              "stepType": "assemble"
            }
          ]
        }
      ]
