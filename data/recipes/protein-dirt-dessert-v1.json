  "versions": [
    {
      "version": 1,
      "status": "final",

      "yield": {
        "servings": 4,
        "unit": "cups"
      },

      "components": [
        {
          "id": "stabilized-whipped-base",
          "title": "Stabilized Whipped Cottage Cheese Base",
          "ingredients": [
            { "name": "Low-fat cottage cheese", "amount": 450, "unit": "g" },
            { "name": "Milk (or unsweetened almond milk)", "amount": 45, "unit": "mL" },
            { "name": "Monk fruit sweetener", "amount": 20, "unit": "g", "note": "or to taste" },
            { "name": "Vanilla extract", "amount": 5, "unit": "mL" },
            { "name": "Salt", "amount": 1, "unit": "g" },
            { "name": "Unflavored gelatin", "amount": 4, "unit": "g" },
            { "name": "Cold water", "amount": 30, "unit": "mL" }
          ],
          "instructions": [
            "Bloom gelatin in cold water for 2–3 minutes.",
            "Heat gently until just liquid (do not boil).",
            "Blend cottage cheese, milk, vanilla, sweetener, and salt until completely smooth.",
            "With blender running, drizzle in gelatin.",
            "Chill mixture 20–30 minutes.",
            "Whip with hand mixer or food processor until light and airy."
          ]
        },

        {
          "id": "chocolate-mousse-layer",
          "title": "Chocolate Pudding Whipped Mousse",
          "ingredients": [
            { "name": "Sugar-free chocolate instant pudding mix", "amount": 24, "unit": "g" },
            { "name": "Cold milk", "amount": 180, "unit": "mL" },
            { "name": "Whipped cottage cheese base", "amount": 240, "unit": "g" }
          ],
          "instructions": [
            "Whisk pudding mix with milk for 2 minutes.",
            "Let thicken for 5 minutes.",
            "Gently fold in whipped cottage cheese base."
          ]
        },

        {
          "id": "vanilla-mousse-layer",
          "title": "Vanilla Pudding Whipped Mousse",
          "ingredients": [
            { "name": "Sugar-free vanilla instant pudding mix", "amount": 24, "unit": "g" },
            { "name": "Cold milk", "amount": 180, "unit": "mL" },
            { "name": "Whipped cottage cheese base", "amount": 240, "unit": "g" },
            { "name": "Almond extract", "amount": 1, "unit": "mL", "optional": true }
          ],
          "instructions": [
            "Whisk pudding mix with milk for 2 minutes.",
            "Let thicken for 5 minutes.",
            "Fold in whipped cottage cheese base and almond extract."
          ]
        },

        {
          "id": "oreo-style-crumb",
          "title": "Baked Oreo-Style Crumb Topping",
          "ingredients": [
            { "name": "Oat flour", "amount": 75, "unit": "g" },
            { "name": "Unsweetened cocoa powder", "amount": 15, "unit": "g" },
            { "name": "Powdered monk fruit sweetener", "amount": 30, "unit": "g" },
            { "name": "Salt", "amount": 1, "unit": "g" },
            { "name": "Butter or coconut oil, melted", "amount": 25, "unit": "g" },
            { "name": "Vanilla extract", "amount": 5, "unit": "mL" },
            { "name": "Espresso powder", "amount": 0.5, "unit": "g", "optional": true }
          ],
          "instructions": [
            "Preheat oven to 165°C (325°F).",
            "Mix all dry ingredients.",
            "Stir in melted fat and vanilla.",
            "Spread thin on parchment-lined tray.",
            "Bake 8–10 minutes, stirring once.",
            "Cool completely, then crumble finely."
          ]
        }
      ],

      "assembly": [
        "Spoon chocolate mousse into serving cups.",
        "Layer vanilla mousse on top.",
        "Finish with Oreo-style crumb topping.",
        "Chill at least 1 hour before serving."
      ],

      "storage": {
        "refrigerated": {
          "optimalDays": 3,
          "maximumDays": 4,
          "notes": "For best texture beyond 3 days, store crumb separately and add before serving."
        }
      }
    }
  ]
}